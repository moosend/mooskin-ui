    "html-to-draftjs": "^1.3.0",
    "input-moment": "^0.4.0",
    "moment": "2.24.0",
    "react-draft-wysiwyg": "https://github.com/donibehrami/react-draft-wysiwyg.git#master",
    "react-live": "1.12.0",
    "react-slick": "^0.23.2",
