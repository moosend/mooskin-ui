{
  "presets": [
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins":  [
    [ "@babel/plugin-proposal-class-properties", { "loose": true } ], 
    "@babel/proposal-object-rest-spread",
    "@babel/plugin-transform-regenerator"
  ],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}